ignore_bin "linux-libc-dev"
ignore_bin "nvidia-*"
ignore_bin "google-chrome-beta"
ignore_bin "picasa"
ignore_bin "w32codecs"

# Binary packages that don't exist in either distro or throw spurious warnings
ignore_bin "mono-git"
ignore_bin "linux-restricted-modules-common"
ignore_bin "ureadahead"
ignore_bin "libstdc++6-4.4-dev"
ignore_bin "dhcdbd"
ignore_bin "libbluetooth2"
ignore_bin "bluez-audio"
ignore_bin "gtkhtml3.14"
ignore_bin "belocs-locales-bin"

# Source for the libraries is now util-linux but these are not the same source
ignore_bin "libblkid1", "libuuid1", "uuid-runtime"
include_src "e2fsprogs"
include_src "util-linux"
include_src "coreutils"

# Ignore all kernel binaries, add "linux" src package to represent the latest one
ignore_bin "linux-image-*-generic"
ignore_bin "linux-headers-*"
include_src "linux"

