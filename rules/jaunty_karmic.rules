# Binary packages that should be collapsed into a single comparison
same_src "firefox-*"
same_src "gcc-*"
same_src "gcj-*"
same_src "readline?"
same_src "glibc", "eglibc"
same_src "xulrunner-*"
same_src "telepathy-mission-control", "telepathy-mission-control-*"
same_src "xfree86-driver-synaptics", "xserver-xorg-input-synaptics"
same_src "ffmpeg-debian", "ffmpeg"
same_src "wxwindows*", "wxwidgets*"
same_src "mysql-dfsg-*"
same_src "postgresql-*"

# Binary packages that are not in Ubuntu
ignore_bin "googleearth"
ignore_bin "virtualbox-3.1"
ignore_bin "adobe-flashplugin"
ignore_bin "skype"
ignore_bin "opera"
ignore_bin "acroread"
ignore_bin "wine1.2"
